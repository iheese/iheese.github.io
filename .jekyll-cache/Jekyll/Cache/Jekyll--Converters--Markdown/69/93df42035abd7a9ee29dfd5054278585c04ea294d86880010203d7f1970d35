<div class="language-java highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="nd">@Getter</span>
<span class="nd">@RequiredArgsConstructor</span>
<span class="kd">public</span> <span class="kd">enum</span> <span class="nc">Detail</span> <span class="o">{</span>

    <span class="no">HIGH_SCHOOL</span><span class="o">(</span><span class="s">"ê³ ë“±í•™ìƒ"</span><span class="o">),</span>
    <span class="no">UNIVERSITY</span><span class="o">(</span><span class="s">"ëŒ€í•™ìƒ"</span><span class="o">),</span>
    <span class="no">SALARY_MAN</span><span class="o">(</span><span class="s">"ì§ì¥ì¸"</span><span class="o">),</span>
    <span class="no">SELF_EMPLOYED</span><span class="o">(</span><span class="s">"ìì˜ì—…ì"</span><span class="o">),</span>
    <span class="no">ETC</span><span class="o">(</span><span class="s">"ê¸°íƒ€"</span><span class="o">),</span>
    <span class="no">CHALLENGING</span><span class="o">(</span><span class="s">"ì·¨ì—…ë„ì „ì¤‘"</span><span class="o">),</span>
    <span class="no">STUDYING</span><span class="o">(</span><span class="s">"ì·¨ì—…ê³µë¶€ì¤‘"</span><span class="o">),</span>
    <span class="no">GIVING_UP</span><span class="o">(</span><span class="s">"í¬ê¸°ìƒíƒœ"</span><span class="o">);</span>

    <span class="kd">private</span> <span class="kd">final</span> <span class="nc">String</span> <span class="n">koreanName</span><span class="o">;</span>
