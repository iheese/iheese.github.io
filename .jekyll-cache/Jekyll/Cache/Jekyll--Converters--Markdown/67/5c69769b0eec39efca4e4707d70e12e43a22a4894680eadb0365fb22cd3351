<span class="kd">class</span> <span class="nc">Main</span> <span class="o">{</span>
  <span class="kd">public</span> <span class="kd">static</span> <span class="kt">void</span> <span class="nf">main</span><span class="o">(</span><span class="nc">String</span><span class="o">[]</span> <span class="n">args</span><span class="o">)</span> <span class="o">{</span>
    
    <span class="nc">Stack</span><span class="o">&lt;</span><span class="nc">Integer</span><span class="o">&gt;</span> <span class="n">stackInt</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">Stack</span><span class="o">&lt;</span><span class="nc">Integer</span><span class="o">&gt;();</span> <span class="c1">//Integer í˜• ìŠ¤íƒ ìƒì„±</span>

    <span class="n">stackInt</span><span class="o">.</span><span class="na">push</span><span class="o">(</span><span class="mi">1</span><span class="o">);</span> <span class="c1">//ì²« ë²ˆì§¸ ê°’ ì…ë ¥</span>
    <span class="n">stackInt</span><span class="o">.</span><span class="na">push</span><span class="o">(</span><span class="mi">2</span><span class="o">);</span> <span class="c1">//ë‘ ë²ˆì§¸ ê°’ ì…ë ¥</span>
    <span class="n">stackInt</span><span class="o">.</span><span class="na">push</span><span class="o">(</span><span class="mi">3</span><span class="o">);</span> <span class="c1">//ì„¸ ë²ˆì§¸ ê°’ ì…ë ¥</span>

    <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="n">stackInt</span><span class="o">);</span> <span class="c1">//[1,2,3]</span>

    <span class="n">stackInt</span><span class="o">.</span><span class="na">pop</span><span class="o">();</span> <span class="c1">//ë§ˆì§€ë§‰ì— ë“¤ì–´ì™”ë˜ ê°’ë¶€í„° ë¹ ì§„ë‹¤.</span>
    <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="n">stackInt</span><span class="o">);</span> <span class="c1">//[1,2]</span>

    <span class="n">stackInt</span><span class="o">.</span><span class="na">pop</span><span class="o">();</span>
    <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="n">stackInt</span><span class="o">);</span> <span class="c1">//[1]</span>

    <span class="n">stackInt</span><span class="o">.</span><span class="na">pop</span><span class="o">();</span>
    <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="n">stackInt</span><span class="o">);</span> <span class="c1">//[]</span>
  <span class="o">}</span>
<span class="o">}</span>

