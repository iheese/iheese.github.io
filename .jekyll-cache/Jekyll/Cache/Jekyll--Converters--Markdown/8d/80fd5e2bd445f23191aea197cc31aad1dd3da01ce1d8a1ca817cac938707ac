    <span class="k">print</span><span class="p">(</span><span class="sa">f</span><span class="s">"Case #</span><span class="si">{</span><span class="n">i</span><span class="o">+</span><span class="mi">1</span><span class="si">}</span><span class="s">: </span><span class="si">{</span><span class="n">ans</span><span class="si">}</span><span class="s">"</span><span class="p">)</span>
